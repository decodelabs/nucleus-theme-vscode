    "tokenColors": [
        // General
        {
            "name": "Colors",
            "scope": ["constant.other.color"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF5370"
            }
        },

        // Keywords
        {
            "name": "Keyword",
            "scope": ["keyword", "storage.modifier"],
            "settings": {
                "foreground": "#80d746ea"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword.control"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage",
                "storage.type.function",
                "storage.type.class",
                "storage.type.interface",
                "storage.type.trait",
                "meta.var storage.type"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#BEB2A0"
            }
        },
        {
            "name": "Methods",
            "scope": [
                "entity.name.function",
                "support.function.magic",
                "meta.function-call",
                "meta.method-call",
                "meta.use constant.other"
            ],
            "settings": {
                "foreground": "#8fcbff"
            }
        },
        {
            "name": "Methods",
            "scope": ["meta.function entity.name.function"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Magic methods",
            "scope": ["meta.function support.function.magic"],
            "settings": {
                "fontStyle": "bold italic"
            }
        },

        // Types
        {
            "name": "Types",
            "scope": ["storage.type"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5affe6df"
            }
        },
        {
            "name": "Namespaces",
            "scope": [
                "entity.name.type.namespace",
                "support.other.namespace",
                "support.other.namespace storage.type"
            ],
            "settings": {
                "foreground": "#49a89ae6"
            }
        },
        {
            "name": "Namespaces",
            "scope": ["support.other.namespace variable.language.namespace"],
            "settings": {
                "foreground": "#eae088c8"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.other.alias",
                "meta.interface"
            ],
            "settings": {
                "foreground": "#5affe6df"
            }
        },
        {
            "name": "Class aliases",
            "scope": ["entity.other.alias"],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Class names",
            "scope": [
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.trait"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#a9d7ff"
            }
        },

        // Punctuation
        {
            "name": "Punctuation",
            "scope": ["punctuation", "keyword.operator", "meta.brace"],
            "settings": {
                "foreground": "#868e90"
            }
        },
        {
            "name": "Subtle punctuation",
            "scope": ["punctuation.separator.inheritance"],
            "settings": {
                "foreground": "#868e9094"
            }
        },
        {
            "name": "Embedded",
            "scope": ["punctuation.section.embedded"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ea7b7b"
            }
        },
        {
            "name": "Type operators",
            "scope": [
                "keyword.operator.type.php",
                "keyword.control.as",
                "keyword.other.use-as",
                "keyword.other.use-insteadof",
                "storage.type.property",
                "keyword.operator.new",
                "keyword.other.new"
            ],
            "settings": {
                "foreground": "#eae088da"
            }
        },

        // Constants
        {
            "name": "Constants",
            "scope": [
                "constant",
                "support.constant",
                "support.type.builtin",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#dbace8"
            }
        },
        {
            "name": "Language constants",
            "scope": ["constant.other"],
            "settings": {
                "foreground": "#fd7acd"
            }
        },
        {
            "name": "Bold constants",
            "scope": ["constant.other", "constant.character.escape"],
            "settings": {
                "fontStyle": "bold"
            }
        },

        // Comments
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#60554a"
            }
        },
        {
            "name": "Comment keywords",
            "scope": ["comment keyword"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#fd7acde1"
            }
        },

        // Variables
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#ced4da"
            }
        },
        {
            "name": "This",
            "scope": ["variable.language.this"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#abdad3"
            }
        },
        {
            "name": "Variable defs",
            "scope": ["punctuation.definition.variable"],
            "settings": {
                "foreground": "#a2a8af"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link",
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#d0bbef"
            }
        },
        {
            "name": "Shell variables",
            "scope": ["source.shell variable", "variable.other.env"],
            "settings": {
                "foreground": "#d4c290"
            }
        },

        // Strings
        {
            "name": "Quotes",
            "scope": ["punctuation.definition.string"],
            "settings": {
                "foreground": "#85625B"
            }
        },
        {
            "name": "String",
            "scope": ["string.quoted", "meta.attribute-selector", "source.env"],
            "settings": {
                "foreground": "#ff9494"
            }
        },
        {
            "name": "Regex",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#eaac8d"
            }
        },
        {
            "name": "Regex anchor",
            "scope": ["keyword.control.anchor.regexp"],
            "settings": {
                "foreground": "#fdf3ef"
            }
        },
        {
            "name": "Regex keyword",
            "scope": ["string.regexp keyword"],
            "settings": {
                "foreground": "#fecccc"
            }
        },
        {
            "name": "Regex char class",
            "scope": [
                "constant.other.character-class.regexp",
                "keyword.operator.quantifier.regexp",
                "constant.other.character-class.range.regexp"
            ],
            "settings": {
                "foreground": "#86e1efc7"
            }
        },
        {
            "name": "Regex char class",
            "scope": ["constant.other.character-class.range.regexp"],
            "settings": {
                "foreground": "#eae188"
            }
        },
        {
            "name": "Template",
            "scope": [
                "string.template",
                "string.other.link.title",
                "string.other.link.description"
            ],
            "settings": {
                "foreground": "#f4c3aa"
            }
        },

        // Declaratives
        {
            "name": "Keys",
            "scope": [
                "meta.object-literal.key",
                "meta.object-literal.key string.quoted",
                "support.type.property-name.json",
                "entity.name.tag.yaml",
                "entity.name.function.neon",
                "support.other.neon"
            ],
            "settings": {
                "foreground": "#b2d7d7c2"
            }
        },
        {
            "name": "Values",
            "scope": [
                "source.neon",
                "string.unquoted.plain",
                "string.quoted.single.yaml",
                "string.quoted.double.yaml",
                "string.quoted.single.neon",
                "string.quoted.double.neon",
                "meta.structure.dictionary string.quoted.double"
            ],
            "settings": {
                "foreground": "#d4c076"
            }
        },

        // Styles
        {
            "name": "Entity",
            "scope": ["entity.name.tag.css"],
            "settings": {
                "foreground": "#5affe6"
            }
        },
        {
            "name": "Unit",
            "scope": ["keyword.other.unit"],
            "settings": {
                "foreground": "#eae188"
            }
        },
        {
            "name": "Class",
            "scope": [
                "entity.other.attribute-name.class",
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffc277"
            }
        },
        {
            "name": "Psuedo",
            "scope": ["entity.other.attribute-name.pseudo-class"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8fcbff"
            }
        },
        {
            "name": "Reference",
            "scope": ["entity.name.tag.reference"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6adc40"
            }
        },
        {
            "name": "Variable",
            "scope": ["source.css variable"],
            "settings": {
                "foreground": "#ed6dbe"
            }
        },

        // Markup
        {
            "name": "Tag begin / end",
            "scope": [
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#d2c4b5"
            }
        },
        {
            "name": "Tag",
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "#b09b7c"
            }
        },
        {
            "name": "Attribute",
            "scope": [
                "meta.attribute",
                "meta.attribute entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#5affe6"
            }
        },
        {
            "name": "Attribute string",
            "scope": ["meta.attribute string.quoted"],
            "settings": {
                "foreground": "#f1bdff"
            }
        },
        {
            "name": "RGB",
            "scope": ["constant.other.rgb-value"],
            "settings": {
                "foreground": "#b79d7e"
            }
        },

        // Markdown
        {
            "name": "Heading",
            "scope": ["markup.heading"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8fcbff"
            }
        },
        {
            "name": "Bold",
            "scope": ["markup.bold"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#f1bdff"
            }
        },
        {
            "name": "Italic",
            "scope": ["markup.italic"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#f1bdff"
            }
        },
        {
            "name": "Quote",
            "scope": ["markup.quote"],
            "settings": {
                "foreground": "#5affe6"
            }
        },
        {
            "name": "List",
            "scope": ["markup.list"],
            "settings": {
                "foreground": "#ab9067"
            }
        },
        {
            "name": "String",
            "scope": [
                "markup.raw.block",
                "markup.inline.raw.string",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#ff9494"
            }
        }
    ]
}
